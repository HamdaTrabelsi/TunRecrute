{% extends 'base.html.twig' %}

{% block title %}Education{% endblock %}

{% block body %}
    <h1>Education</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ education.id }}</td>
            </tr>
            <tr>
                <th>DiplomaTitle</th>
                <td>{{ education.DiplomaTitle }}</td>
            </tr>
            <tr>
                <th>InstitutionName</th>
                <td>{{ education.InstitutionName }}</td>
            </tr>
            <tr>
                <th>StartYear</th>
                <td>{{ education.startYear }}</td>
            </tr>
            <tr>
                <th>EndYear</th>
                <td>{{ education.endYear }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ education.Description }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('education_index') }}">back to list</a>

    <a href="{{ path('education_edit', {'id': education.id}) }}">edit</a>

    {{ include('education/_delete_form.html.twig') }}
